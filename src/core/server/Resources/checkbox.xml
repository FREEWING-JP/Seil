<?xml version="1.0"?>
<root>
  <item>
    <name>Caps Lock</name>
    <expand>true</expand>
    <item>
      <name>Change Caps Lock</name>
      <enable>enable_capslock</enable>
      <keycode>keycode_capslock</keycode>
      <kHIDUsage>kHIDUsage_KeyboardCapsLock</kHIDUsage>
      <default>51</default>
    </item>
  </item>

  <item>
    <name>Other Keys</name>
    <item>
      <name>Note:</name>
      <appendix>These items can be useful if any of the following:</appendix>
      <appendix>* If you want to swap Caps Lock and other key.</appendix>
      <appendix>* If you're using Sticky Keys in Universal Access.</appendix>
      <appendix></appendix>
      <appendix>Otherwise, use Karabiner to modify these keys.</appendix>
    </item>
    <item><name>────────────────────</name></item>
    <item>
      <name>Change Command_L</name>
      <enable>enable_command_l</enable>
      <keycode>keycode_command_l</keycode>
      <kHIDUsage>kHIDUsage_KeyboardLeftGUI</kHIDUsage>
      <default>55</default>
    </item>
    <item>
      <name>Change Command_R</name>
      <enable>enable_command_r</enable>
      <keycode>keycode_command_r</keycode>
      <kHIDUsage>kHIDUsage_KeyboardRightGUI</kHIDUsage>
      <default>54</default>
    </item>
    <item>
      <name>Change Control_L</name>
      <enable>enable_control_l</enable>
      <keycode>keycode_control_l</keycode>
      <kHIDUsage>kHIDUsage_KeyboardLeftControl</kHIDUsage>
      <default>59</default>
    </item>
    <item>
      <name>Change Control_R</name>
      <enable>enable_control_r</enable>
      <keycode>keycode_control_r</keycode>
      <kHIDUsage>kHIDUsage_KeyboardRightControl</kHIDUsage>
      <default>62</default>
    </item>
    <!-- fn key is not in _usb_2_adb_keymap -->
    <item>
      <name>Change Option_L</name>
      <enable>enable_option_l</enable>
      <keycode>keycode_option_l</keycode>
      <kHIDUsage>kHIDUsage_KeyboardLeftAlt</kHIDUsage>
      <default>58</default>
    </item>
    <item>
      <name>Change Option_R</name>
      <enable>enable_option_r</enable>
      <keycode>keycode_option_r</keycode>
      <kHIDUsage>kHIDUsage_KeyboardRightAlt</kHIDUsage>
      <default>61</default>
    </item>
    <item>
      <name>Change Shift_L</name>
      <enable>enable_shift_l</enable>
      <keycode>keycode_shift_l</keycode>
      <kHIDUsage>kHIDUsage_KeyboardLeftShift</kHIDUsage>
      <default>56</default>
    </item>
    <item>
      <name>Change Shift_R</name>
      <enable>enable_shift_r</enable>
      <keycode>keycode_shift_r</keycode>
      <kHIDUsage>kHIDUsage_KeyboardRightShift</kHIDUsage>
      <default>60</default>
    </item>
    <item><name>────────────────────</name></item>
    <item>
      <name>Change Delete</name>
      <enable>enable_delete</enable>
      <keycode>keycode_delete</keycode>
      <kHIDUsage>kHIDUsage_KeyboardDeleteOrBackspace</kHIDUsage>
      <default>51</default>
    </item>
    <item>
      <name>Change Escape</name>
      <enable>enable_escape</enable>
      <keycode>keycode_escape</keycode>
      <kHIDUsage>kHIDUsage_KeyboardEscape</kHIDUsage>
      <default>53</default>
    </item>
    <item>
      <name>Change Keypad Enter</name>
      <enable>enable_enter</enable>
      <keycode>keycode_enter</keycode>
      <kHIDUsage>kHIDUsage_KeypadEnter</kHIDUsage>
      <default>76</default>
    </item>
    <item>
      <name>Change Return</name>
      <enable>enable_return</enable>
      <keycode>keycode_return</keycode>
      <kHIDUsage>kHIDUsage_KeyboardReturnOrEnter</kHIDUsage>
      <default>36</default>
    </item>
    <item>
      <name>Change Tab</name>
      <enable>enable_tab</enable>
      <keycode>keycode_tab</keycode>
      <kHIDUsage>kHIDUsage_KeyboardTab</kHIDUsage>
      <default>48</default>
    </item>
  </item>

